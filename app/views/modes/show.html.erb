<div class="page-header">
  <h3>Mode #<%= @mode.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Mode</dt>
      <dd><%= @mode.mode %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/modes" class="btn btn-primary">
        Back
      </a>
      <a href="/modes/<%= @mode.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_mode/<%= @mode.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
